<h1>Posts#new</h1>
<p>Find me in app/views/posts/new.html.erb</p>
<h1>画像投稿フォーム</h1>
  <%= form_with model: @post, local: true do |f| %>
    <h4>画像</h4>
      <%= f.file_field :images, multiple: true, direct_upload: true, required: true, accept: "image/*" %>
    <h4>家具の名前</h4>
      <%= f.text_field :furniture_name %>
    <h4>説明</h4>
      <%= f.text_area :caption %>
    <%= f.submit '投稿' %>
  <% end %>